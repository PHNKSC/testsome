-- StarterGui.ScreenGui.Frame.SLIDER🔰FUNCTION🔰.LocalScript
local function C_4()
local script = G2L["4"];
	--[[
	
	--save พัง****
	
	
	
	local mouse = game.Players.LocalPlayer:GetMouse()
	local sliderz = script.Parent
	
	function UPDslider()
		local output = (Vector2.new(mouse.X,mouse.Y)-sliderz.AbsolutePosition)/sliderz.AbsoluteSize
		
		print(output)
		
	end
	
	
	]]
	
	
	
	
	
	
	
	--แก้ละ
	local title = script.Parent.process
	
	
	title.Text = "ROTATE HEAD : 0"
	
	local mo = game.Players.LocalPlayer:GetMouse()
	local sder = script.Parent
	local trigger = script.Parent.checkk
	local pzo = script.Parent.pros
	local val = script.Parent.Value
	
	
	local nubss = 0
	
	
	pzo.Size = UDim2.fromScale(val.Value,1)
	--title.Text = tostring(math.round(val.Value*100))
	
	
	
	
	local TweenService = game:GetService("TweenService")
	local tweenstyle = TweenInfo.new(0.5,Enum.EasingStyle.Elastic)
	
	
	
	function UpdateSlider()
		local output = math.clamp((mo.X-sder.AbsolutePosition.X)/sder.AbsoluteSize.X,0,1)
		
		title.Text = "ROTATE HEAD : " .. tostring(math.round(output*100))
		val.Value = output
		
		--///////////////////////////
		--FUNCTION HERE
		--VALUE USE > output * number
		
		
	--	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = output * 100
		
		
		nubss = output * 100
		
		local args = {
			[1] = "Head",
			[2] = CFrame.new(0, 0, 0, 1, 0, 0, 0, 0.7376635074615479, -0.675168514251709, 0, 0.675168514251709, nubss)
		}
	
		game:GetService("ReplicatedStorage"):WaitForChild("PoseEditorDependencies"):WaitForChild("ChangeTransformationForPart"):FireServer(unpack(args))
		
		
		
		
		
		
		
		
		
		
	
		--FUNCTION HERE
		--\\\\\\\\\\\\\\\\\\\\\\\\\\\
	
		if val.Value ~= output then
			TweenService:Create(pzo,tweenstyle,{Size = UDim2.fromScale(output,1)}):Play()
		end
		
		
	end
	
	local Slider = false
	
	function ActivateSlider()
		Slider = true
		while Slider do
			UpdateSlider()
			task.wait()
		end
	end
	
	trigger.MouseButton1Down:Connect(ActivateSlider)
	
	game:GetService("UserInputService").InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			Slider = false
			
		end
	end)
	
	
end;
task.spawn(C_4);
-- StarterGui.ScreenGui.Frame.SLIDER🔰FUNCTION🔰.pros.UIGradient.LocalScript
